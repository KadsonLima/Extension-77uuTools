{
    "name": "77uu Tools",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "./assets/img/logoicon.png",
        "icons": {
           "16": "./assets/img/logoicon.png",
           "48": "./assets/img/logoicon.png",
          "128": "./assets/img/logoicon.png"
        },
        "default_popup": "popup.html"
      },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["./assets/css/style.css"],
        "js": ["content.js"]
      }
    ]
  }